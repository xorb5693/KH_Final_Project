<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="buyProduct">
	<insert id="insertBuyProduct" parameterType="map">
		
		
		
		<foreach collection="list" item="li">
		insert into buy_product (buy_no,pno,stock) values
			(#{li.buyNo},#{li.pno},#{li.stock})
			
		</foreach>

	</insert>
  
</mapper>
